{
  "name": "metamodelica",
  "displayName": "MetaModelica",
  "description": "Modelica and MetaModelica language support including syntax highlighting, bracket matching and some useful snippets",
  "version": "0.0.1",
  "publisher": "AnHeuermann",
  "license": "See LICENSE.txt",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "keywords": [
    "modelica",
    "language",
    "metamodelica"
  ],
  "homepage": "https://github.com/AnHeuermann/modelica-language-vscode",
  "repository": {
    "type": "git",
    "url": "https://github.com/AnHeuermann/modelica-language-vscode.git"
  },
  "icon": "images/icon.png",
  "bugs": "https://github.com/AnHeuermann/modelica-language-vscode/issues",
  "contributes": {
    "languages": [
      {
        "id": "modelica",
        "aliases": [
          "Modelica",
          "modelica"
        ],
        "extensions": [
          ".mo",
          ".mos"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "optimica",
        "aliases": [
          "Optimica",
          "optimica"
        ],
        "extensions": [
          ".mop"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "metamodelica",
        "aliases": ["MetaModelica", "metamodelica"],
        "extensions": [".mo"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "modelica",
        "scopeName": "source.modelica",
        "path": "./syntaxes/modelica.tmLanguage"
      },
      {
        "language": "optimica",
        "scopeName": "source.optimica",
        "path": "./syntaxes/optimica.tmLanguage"
      },
      {
        "language": "metamodelica",
        "injectTo": ["modelica"],
        "scopeName": "source.metamodelica",
        "path": "./syntaxes/metamodelica.tmLanguage"
      }
    ],
    "snippets": [
      {
        "language": "modelica",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "metamodelica",
        "path": "./snippets/snippets.json"}
    ]
  }
}
