  },
  "connect": {
    "prefix": "connect",
    "body": "\nconnect(${1:connector1}, ${2:connector2});\n",
    "description": "Modelica connect-equation",
    "scope": "source.modelica"
  },
  "connector": {
    "prefix": "connector",
    "body": "\nconnector ${1}\n  ${2}\n  flow ${3};\nend ${1};\n",
